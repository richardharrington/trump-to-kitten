{
    "manifest_version": 2,
    "name": "Trump to Kitten",
    "short_name": "T2K",
    "version": "1.0.1",
    "description": "Replaces pictures of Trump with pictures of kittens",
    "icons": {
        "48": "icons/ttk-48.png",
        "96": "icons/ttk-96.png"
    },
    "content_scripts":
    [
        {
            "matches": ["*://*/*"],
            "match_about_blank": true,
            "js": ["content_script.js"],
            "all_frames": true
        }
    ]
}
